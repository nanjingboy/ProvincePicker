<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/province_picker_item_radius"/>
            <stroke android:width="1dp" android:color="@android:color/white" />
            <solid android:color="@color/colorProvincePickerItemBackground" />
        </shape>
    </item>
    <item android:top="1dp" android:right="1dp" android:bottom="1dp" android:left="1dp">
        <shape android:shape="rectangle">
            <corners android:radius="4dp"/>
            <stroke android:width="1dp" android:color="@color/colorProvincePickerItemBackground" />
            <solid android:color="@color/colorProvincePickerItemBackground" />
        </shape>
    </item>
</layer-list>